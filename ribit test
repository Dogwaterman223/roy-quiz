def main():
    print("Welcome to the Robot quiz!")
    print("1. Option 1 - Robot is a bad person!")
    print("2. Option 2 - Robot is a nice person!")

    choice = input("Enter your choice (1 or 2): ")

    if choice == "1":
        print("You chose Option 1 - I don't like you")
    elif choice == "2":
        print("You chose Option 2 - good choice!")
    else:
        print("Invalid choice. Please enter 1 or 2.")

if __name__ == "__main__":
    main()

