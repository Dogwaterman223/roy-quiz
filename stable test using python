def main():
    print("Welcome to the Roy Evaluation Program!")
    print("1. Option 1 - Bad for Roy")
    print("2. Option 2 - Bad for Roy")

    choice = input("Enter your choice (1 or 2): ")

    if choice == "1":
        print("You chose Option 1 - Bad for Roy")
    elif choice == "2":
        print("You chose Option 2 - Bad for Roy")
    else:
        print("Invalid choice. Please enter 1 or 2.")

if __name__ == "__main__":
    main()
